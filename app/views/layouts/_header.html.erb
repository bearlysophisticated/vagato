<%=
    if user_signed_in?
        case current_user.role?
          when 'Guest'
            render 'layouts/header_guest'
          when 'Owner'
            render 'layouts/header_owner'
          when 'Admin'
            render 'layouts/header_admin'
          else
            render 'layouts/header_public'
        end
    else
        render 'layouts/header_public'
    end
%>